<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jackz">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Apptivity"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.SecondActivity"/>
        <activity android:name=".activities.LocationsActivity"/>
        <activity android:name=".activities.InfoActivity"/>
        <activity android:name=".activities.StartedActivity"/>
        <activity android:name=".activities.SearchTypeActivity"/>
        <activity android:name=".activities.SavedSearchActivity"/>
        <activity android:name=".activities.ActivitiesQuestions"/>
    </application>

</manifest>